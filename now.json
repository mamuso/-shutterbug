{
  "version": 2,
  "env": {
    "POSTGRES_URL": "@postgres_url",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "POSTGRES_URL": "@postgres_url",
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "api/src/index.ts",
      "use": "@now/node",
      "config": { "maxLambdaSize": "20mb" }
    }
  ],
  "routes": [{ "src": "/api", "dest": "/api/src/index.ts" }]
}
